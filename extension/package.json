{
  "name": "app",
  "version": "0.1.0",
  "homepage": "https://app.wika.network/",
  "private": true,
  "dependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "1.0.1",
    "@mui/icons-material": "5.6.2",
    "@mui/material": "5.6.2",
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@fontsource/roboto": "4.5.5",
    "@polkadot/wasm-crypto": "4.5.1",
    "@polkadot/wasm-crypto-asmjs": "4.5.1",
    "@polkadot/wasm-crypto-wasm": "4.5.1",
    "@polkadot/react-identicon": "0.83.1"
  },
  "scripts": {
    "background": "cp ../background/build/background.js ./public/",
    "commons": "rm -rf ./src/commons && cp -R ../commons ./src/commons",
    "react": "react-scripts build",
    "build": "npm run background && npm run commons && npm run react"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
