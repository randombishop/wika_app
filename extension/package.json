{
  "name": "ext",
  "version": "0.1.0",
  "homepage": "https://app.wika.network/",
  "private": true,
  "scripts": {
    "background": "rm -rf ./src/background && cp -R ../webapp/src/background ./src/background && browserify -p esmify ./src/background/background_ext.js > public/background_ext.js",
    "ux": "rm -rf ./src/ux && cp -R ../webapp/src/ux ./src/ux",
    "react": "react-scripts build",
    "build": "npm run background && npm run ux && npm run react"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@fontsource/roboto": "4.5.5",
    "@mui/icons-material": "5.6.2",
    "@mui/material": "5.6.2",
    "@polkadot/api": "8.4.2",
    "@polkadot/extension-dapp": "0.43.2",
    "@polkadot/react-identicon": "2.2.1",
    "crypto-js": "4.1.1",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-scripts": "5.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "browserify": "^17.0.0",
    "esmify": "^2.1.1",
    "gh-pages": "^3.2.3"
  }
}
